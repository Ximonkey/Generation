package org.generation.collections;

import java.util.HashMap;

public class HashMapCollection {

	public static void main(String[] args) {
		/*
		 * HashMap almacena elementos en pares "clave/valor" y se puede acceder a ellos mediante index.
		 * No ordenado y permite datos duplicados
		 * */
		HashMap<String, String> capitals = new HashMap<String, String>();
		
		//Agregamos elementos "clave/valor" (.put)
		capitals.put("Villahermosa", "Tabasco");
		capitals.put("Monterrey, "Nuevo leon");
		capitals.put(null, null);
		capitals.put(null, null);
		capitals.put(null, null);
		capitals.put(null, null);
		capitals.put(null, null);
	}

}
